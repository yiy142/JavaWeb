<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.Users" table="Users" schema="mydb">
        <id name="sid">
            <column name="sid" sql-type="int(11)"/>
        </id>
        <property name="userName">
            <column name="UserName" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="password">
            <column name="Password" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>